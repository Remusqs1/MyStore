{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": 0,
    "quotes": [
      "off",
      "single"
    ],
    "dot-notation": "off",
    "eqeqeq": "warn",
    "brace-style": "off",
    "space-before-function-paren": [
      "off",
      "always"
    ],
    "no-unused-vars": "warn",
    "no-empty": 1,
    "no-empty-function": 1, //warn
    "padded-blocks": 0,
    "indent": 0, //off
    "no-trailing-spaces": 0,
    "no-multiple-empty-lines": 1,
    "no-multi-spaces": 1,
    "key-spacing": 1,
    "eol-last": 0,
    "prefer-const": 1,
    "space-before-blocks": 0,
    "spaced-comment": 0
  }
}